<% head = @projeto.nome%>
<% provide(:title, "Projetos") %>
<% provide(:header, head) %>
<main>
  <p id="notice"><%= notice %></p>
  <section>
    <div class="row">
      <div id="form-show" class="col-xs-12 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3">

        <label>Nome</label>
        <p>
          <%= @projeto.nome %>
        </p>

        <label>Codigo base</label>
        <p>
          <%= @projeto.codigo_base %>
        </p>

        <label>Revisão</label>
        <p>
          <%= @projeto.revisao %>
        </p>

        <label>Data</label>
        <p>
          <%= @projeto.data %>
        </p>

        <label>Usuario</label>
        <p>
          <%= @projeto.usuario %>
        </p>

        <label>Observações</label>
        <p>
          <%= @projeto.observacoes %>
        </p>

        <label>Número de habitantes</label>
        <p>
          <%= @projeto.numero_habitantes %>
        </p>

        <label>Localização</label>
        <p>
          <%= @projeto.localizacao %>
        </p>

        <label>Tipo do projeto</label>
        <p>
          <%= @projeto.tipo_projeto %>
        </p>
      </div>
    </div>
  </section>
  <section>
    <div class="row">
      <div class="col-xs-12 col-sm-6 col-sm-offset-3 col-md-4 col-md-offset-4">
        <%= link_to 'Edit', edit_projeto_path(@projeto), :class => "btn btn-default"%>
        <%= link_to 'Back', projetos_path, :class => "btn btn-default" %>
      </div>
    </div>
  </section>
</main>
